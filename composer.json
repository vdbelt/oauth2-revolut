{
    "name": "vdbelt/oauth2-revolut",
    "description": "Revolut Provider for OAuth 2.0 Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin van de Belt",
            "email": "martin@vandebelt.dk"
        }
    ],
    "require": {
        "league/oauth2-client": "^2.6",
        "lcobucci/jwt": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "League\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\OAuth2\\Client\\Test\\": "test/src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 || ^11.0 || ^12.0",
        "squizlabs/php_codesniffer": "^3.13",
        "mockery/mockery": "^1.6",
        "ext-json": "*"
    }
}
